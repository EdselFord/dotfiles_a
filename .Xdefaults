!! URxvt Restore Copy Paste
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false

urxvt.perl-ext-common: default,tabbed,matcher,resize-font,-tabbed
!!URxvt.perl-ext-common: default,matcher,tabbed,font-size

URxvt.keysym.Control-Up: resize-font:bigger
URxvt.keysym.Control-Down: resize-font:smaller
URxvt.keysym.Control-equal: resize-font:bigger
URxvt.keysym.Control-question: resize-font:reset


!! URxvt Link Click
urxvt*perl-lib:            /usr/lib/urxvt/perl/
urxvt*perl-ext-common:        default,matcher
urxvt*urlLauncher:        /usr/bin/firefox
urxvt*matcher.button:        1
urxvt*matcher.pattern.1:   \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]






Nsxiv.window.background: #0F0F17
Nsxiv.window.foreground: #efefef
!Nsxiv.window.alpha













!! XTerm configuration
XTerm.vt100.faceName: JetBrains Mono Nerd Font:regular:size=10
XTerm.vt100.foreground: rgb:ef/ef/ef
XTerm.vt100.background: rgb:0F/0F/17

XTerm.vt100.geometry: 92x24
XTerm.vt100.internalBorder: 10


!! XTerm shortcut
XTerm.vt100.translations: #override \n\
		Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
		Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\
		Ctrl <Key>+:larger-vt-font() \n\
		Ctrl <Key>-:smaller-vt-font() \n\

